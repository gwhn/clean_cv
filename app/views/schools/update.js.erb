<%- schools_id = "person_#{@person.id}_schools" %>
$("#<%= schools_id %> #school_<%= @school.id %>").replaceWith("<%= escape_javascript(render :partial => 'item', :object => @school) %>");
$("#<%= schools_id %> .school:last .item").addClass("last-item");
bindRemoteLinks("#form-dialog", "#<%= schools_id %> #school_<%= @school.id %> .remote-link");
$("#<%= schools_id %> #school_<%= @school.id %>").effect("highlight");
