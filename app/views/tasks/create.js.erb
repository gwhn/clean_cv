<%- tasks_id = "#project_#{@project.id}_tasks" -%>
<%- task_id = "#task_#{@task.id}" -%>
$("<%= escape_javascript(render :partial => 'item', :object => @task) %>").appendTo("<%= tasks_id %>");
bindRemoteLinks("#form-dialog", ".remote-link");
$(".move-actions, .update-actions").hide();
<%= show_update_actions_javascript_for "#{task_id}", :on_ready_wrapper => false %>
$("<%= task_id %>").effect("highlight");

