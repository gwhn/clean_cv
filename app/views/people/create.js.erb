<%- person_id = "#person_#{@person.id}" -%>
$("<%= escape_javascript(render :partial => 'item', :object => @person) %>").appendTo("#people");
<%= dynamic_tooltip_javascript_for "#{person_id} > .personal", 'top right' %>
initFancyBox("a.lightbox");
bindRemoteLinks("#form-dialog", ".remote-link");
$("<%= person_id %>").effect("highlight");
